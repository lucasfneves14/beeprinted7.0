<script>

$( document ).ready(function() {
  
  $('.cor').each(function(index, element) {
    console.log(parseFloat($(this).text()));
    if(parseFloat($(this).text()) < 100){
      $(this).css('color', 'red');
    }
    else{
      $(this).css('color', 'green');
    }
    
  });
  $('.cor2').each(function(index, element) {
    console.log(parseFloat($(this).text()));
    if(parseFloat($(this).text()) < 0){
      $(this).css('color', 'red');
    }
    else{
      $(this).css('color', 'green');
    }
    
  });

  $(".integral").click(function(){
    parcial = $(this).next();
    console.log(parcial)
    if (parcial.is(":visible")){
      parcial.fadeOut();
      parcial.next().fadeOut();
      if ( parcial.next().next().hasClass( "pessoa" ) ) {
        parcial.next().next().fadeOut();
      }
    }
    else{
      parcial.fadeIn();
      parcial.next().fadeIn();
      if ( parcial.next().next().hasClass( "pessoa" ) ) {
        parcial.next().next().fadeIn();
      }

    }
  });


});

</script>